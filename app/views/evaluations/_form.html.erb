<%= simple_form_for(@evaluation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.input :procedure_id %>
    <%= f.input :evaluation_form_id %>
    <%= f.input :evaluator_id %>
    <%= f.input :resident_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
