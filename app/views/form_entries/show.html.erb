<%= page_title("#{@form.name}") do %>
  <%= link_to "Delete", form_form_entry_path(@form, @entry), method: :delete, data: { confirm: 'Are you sure?' }, class: 'pull-right btn btn-danger' %>
  <%= link_to "Edit", edit_form_form_entry_path(@form, @entry), class: 'pull-right btn btn-default' %>
<% end %>
<div class="entry index"><%= "Viewing Entry #"+@number.to_s %></div>
<pre><%= JSON.pretty_generate JSON.parse(@entry.to_json) %></pre>