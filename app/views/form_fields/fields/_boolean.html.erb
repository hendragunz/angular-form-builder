<% labels = eval(question.properties['data']) %>
<label class="radio-inline">
  <%= f.radio_button question.id.to_s, true %> <%= labels['true_label'].present? ? labels['true_label'] : "Yes" %>
</label>

<label class="radio-inline">
  <%= f.radio_button question.id.to_s, false %> <%= labels['false_label'].present? ? labels['false_label'] : "No" %>
</label>