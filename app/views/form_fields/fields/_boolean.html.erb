<label class="radio-inline">
  <%= f.radio_button question.id.to_s, true %> <%= question.true_label.present? ? question.true_label : "Yes" %>
</label>

<label class="radio-inline">
  <%= f.radio_button question.id.to_s, false %> <%= question.false_label.present? ? question.false_label : "No" %>
</label>